{
  "name": "react-webview-extension",
  "displayName": "React Webview Extension",
  "description": "A simple VS Code extension using React in Webview.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "activationEvents": [
    "onCommand:extension.showReactWebview",
    "onCommand:extension.searchStackOverflow",
    "onCommand:extension.showTreeInTerminal"
  ],
  "main": "extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.showReactWebview",
        "title": "Show React Webview"
      },
      {
        "command": "extension.searchStackOverflow",
        "title": "Search Stack Overflow"
      },
      {
        "command": "extension.showTreeInTerminal",
        "title": "Show Folder Tree in Terminal"
      }
    ],
    "keybindings": [
      {
        "command": "extension.searchStackOverflow",
        "key": "ctrl+0",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.searchStackOverflow",
        "key": "cmd+0",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.showTreeInTerminal",
        "key": "ctrl+alt+t",
        "mac": "cmd+alt+t",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack",
    "watch": "webpack --watch"
  },
  "categories": [
    "Other"
  ],
  "dependencies": {
    "jspdf": "^3.0.1",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "style-loader": "^4.0.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
